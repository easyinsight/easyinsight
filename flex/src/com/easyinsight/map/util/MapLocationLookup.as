		public function getLocation(name:String, mapType:String):String {
			var aliasMap:Object = getAliasMap(mapType);
			var location:String = aliasMap[name.toUpperCase()];
            if (location == null) {
                return name;
            } else {
                return location;
            }
		}

        
		
		private function getAliasMap(mapType:String):Object {
			var aliasMap:Object;
			switch (mapType) {
				case "1":
					aliasMap = usAliasMap;
					break;
				case "2":
					aliasMap = worldAliasMap;
					break;
				case "3":
					aliasMap = europeAliasMap;
					break;
				case "4":
					aliasMap = asiaAliasMap;
					break;
				case "5":
					aliasMap = americasAliasMap;
					break;
				case "6":
					aliasMap = middleEastAliasMap;
					break;
			}
			return aliasMap;
		}

