<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%" xmlns:collaboration="com.easyinsight.collaboration.*">
	<mx:Script>
		<![CDATA[
			import mx.messaging.events.MessageEvent;
			
			[Bindable]
			private var _sessionName:String;
			
			public function set sessionName(sessionName:String):void {
				this._sessionName = sessionName;
			}
		]]>
	</mx:Script>
	
	<mx:HBox width="100%" height="100%">
		<collaboration:SessionAttendeeList/>
		<collaboration:Chat sessionName="{_sessionName}"/>
	</mx:HBox>
</mx:Canvas>
