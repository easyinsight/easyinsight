            var k:CrunchbaseSolutionDetail;
            var l:ConstantContactSolutionDetail;
            var m:QuickbaseSolutionDetail;
            var n:ClearDBSolutionDetail;
            var o:BatchbookSolutionDetail;
        }

        public function gotFocus():void {
