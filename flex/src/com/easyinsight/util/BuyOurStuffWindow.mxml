    <mx:HBox>
        <mx:Button label="Set up Billing Information Now" click="flash.net.navigateToURL(new URLRequest('billing/billing.jsp'), '_self')" />
        <mx:Button label="Set up Later" click="PopUpManager.removePopUp(this)" />
    </mx:HBox>
    <mx:Script><![CDATA[
        import flash.net.URLRequest;

        import mx.managers.PopUpManager;
        ]]></mx:Script>
</mx:TitleWindow>