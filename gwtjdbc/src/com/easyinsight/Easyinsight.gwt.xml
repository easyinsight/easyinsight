<module rename-to="Easyinsight">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.easyinsight.client.DBCore'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/DBService' class='com.easyinsight.dbclient.DBServiceImpl'/>

</module>