<?xml version="1.0"?>
<reportviews:AbstractedListView xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
            xmlns:reportviews="com.easyinsight.reportviews.*" implements="com.easyinsight.reportviews.IReportView" height="100%">
    <fx:Script><![CDATA[
        import com.easyinsight.analysis.AnalysisDefinition;
        import com.easyinsight.analysis.list.ListDefinition;

        import mx.collections.ArrayCollection;

        public function renderReport(data:ArrayCollection, report:AnalysisDefinition, additionalProperties:Object):void {
            var listDef:ListDefinition = report as ListDefinition;
            populateGrid(listDef.columns, listDef, data);
        }

        public function preserveValues():Boolean {
            return true;
        }
        ]]></fx:Script>
</reportviews:AbstractedListView>